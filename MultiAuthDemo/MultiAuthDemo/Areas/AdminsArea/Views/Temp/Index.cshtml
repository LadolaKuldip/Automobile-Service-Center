@model IEnumerable<ASC.Entities.Customer>
@{
    ViewBag.Title = "Index";
}



<div class="container">
    <div class="row justify-content-center">
        <div class="col-lg-7">
            <div class="card shadow-lg border-0 rounded-lg mt-5">
                <div class="card-header"><h3 class="text-center font-weight-light my-4">Create Brand</h3></div>
                <div class="card-body">
                    <form>

                        <div class="form-group">
                            <label class="small mb-1" for="inputEmailAddress">Email</label>
                            <input class="form-control py-4" id="inputEmailAddress" type="email" aria-describedby="emailHelp" placeholder="Enter email address" />
                        </div>
                        <div class="form-group mt-4 mb-0">
                            <a class="btn btn-primary btn-block" href="login.html">Create Account</a>
                        </div>
                    </form>
                </div>

            </div>
        </div>
    </div>
</div>

